{
  "name": "postgres-datasource",
  "type": "postgres",
  "typeLogoUrl": "public/app/plugins/datasource/postgres/img/postgresql_logo.svg",
  "access": "proxy",
  "url": "$postgres_hostname:$postgres_port",
  "secureJsonData": {
    "password": "$postgres_password"
  },
  "user": "$postgres_user",
  "database": "$postgres_db",
  "basicAuth": false,
  "isDefault": true,
  "jsonData": {
    "sslmode": "disable"
  }
}